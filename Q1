#include<stdio.h>
int main()
{
   int n,k,c=0;
   printf("Enter key\n");
   scanf("%d",&k);
   printf("enter the no. of elements\n");
   scanf("%d",&n);
   int arr[n];
   printf("Enter the numbers\n");
   for(int i=0;i<n;i++)
   {
   scanf("%d",&arr[i]);
   }
   for(int i=0;i<n;i++)
   {
   if(arr[i]==k)
   c++;
   }
   if(c==0)
   printf("Key not found");
   else
   printf("key is found %d times",c);
    return 0;
}
